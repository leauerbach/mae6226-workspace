    "def create_meshgrid(N,x_start,x_end,y_start,y_end):\n",
    "    x = numpy.linspace(x_start,x_end,N)\n",
    "    y = numpy.linspace(y_start,y_end,N)\n",
    "    X, Y = numpy.meshgrid(x,y)\n",
    "    \n",
    "    return x, y, X, Y"
